{
  "version": 2,
  "headers": [
    {
      "source": "/(.*\\.php)",  // Target semua file .php
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.php"  // Arahkan root path ke index.php
    }
  ]
}
